    <!-- Prior probability density on the value of the parameter &apos;strictMean_of_shared_clock&apos;, according to a uniform distribution with lower=0, upper=10. -->
    <prior id="prP_UniformDistrib_on_param_strictMean_of_shared_clock" name="distribution" x="@strictMean_of_shared_clock">
     <Uniform id="UniformDistrib_on_param_strictMean_of_shared_clock" name="distr" offset="0" lower="0" upper="10"/>
    </prior>
    
    <!-- Prior on parameter(s) for morphology evolution model -->
    
    <!-- Prior density of the gamma shape parameter for morphology -->
    <prior id="morph_gammaShape_prior" distr="@exponential_distrib_for_gammaShape_priors" name="distribution" x="@morph_gammaShape"/>
    
    <!-- Prior probability distributions of tree model parameters -->
    
    <!-- Uniform prior on the time of origin of the process (assumes my) -->
    <!-- x means whatever parameter you are assessing via the distribution -->
    <prior id="prior_originTime" name="distribution" x="@originTime">
     <Uniform id="prior_originTime_Uniform_distrib" lower="0.0" upper="4600.0" name="distr"/>
    </prior>
    
    <!-- Priors on birthRate(s) -->
    
    <!-- Prior probability density on the value of the parameter &apos;birthRate&apos;, according to a uniform distribution with lower=0, upper=9. -->
    <prior id="prP_UniformDistrib_on_param_birthRate" name="distribution" x="@birthRate">
     <Uniform id="UniformDistrib_on_param_birthRate" name="distr" offset="0" lower="0" upper="9"/>
    </prior>
    
    <!-- Priors on deathRate(s) -->
    
    <!-- Prior probability density on the value of the parameter &apos;deathRate&apos;, according to a uniform distribution with lower=0, upper=9. -->
    <prior id="prP_UniformDistrib_on_param_deathRate" name="distribution" x="@deathRate">
     <Uniform id="UniformDistrib_on_param_deathRate" name="distr" offset="0" lower="0" upper="9"/>
    </prior>
    
    <!-- Prior on samplingRate(s) -->
    
    <!-- Prior probability density on the value of the parameter &apos;samplingRate&apos;, according to a uniform distribution with lower=0, upper=9. -->
    <prior id="prP_UniformDistrib_on_param_samplingRate" name="distribution" x="@samplingRate">
     <Uniform id="UniformDistrib_on_param_samplingRate" name="distr" offset="0" lower="0" upper="9"/>
    </prior>
    
    <!-- Also: we are assuming for now that rho at time 0 is fixed -->
    
    <!-- Probability of MCMC sampled tree given treeModel parameters -->
    <distribution id="BDSKY_prior_shared_tree" spec="beast.evolution.speciation.BirthDeathSkylineModel" tree="@shared_tree" origin="@originTime" birthRate="@birthRate" birthRateChangeTimes="@birthRateChangeTimes_tops" deathRate="@deathRate" deathRateChangeTimes="@deathRateChangeTimes_tops" samplingRate="@samplingRate" samplingRateChangeTimes="@samplingRateChangeTimes_tops" rho="@rho" rhoSamplingTimes="@rhoSamplingTimes" reverseTimeArrays="@reverseTimeArrays"/>
   </distribution>
   <distribution id="likelihood" spec="util.CompoundDistribution">
    
    
    <!-- XML SECTION 13: Likelihoods -->
    
    
    <!-- idrefs for additional likelihoods -->
    
    <!-- Likelihood of morphology data given tree, model, morphology model parameters -->
    
    <!-- Likelihood of Thaleops_v2simp_morph2_unordered on the tree -->
    <distribution idref="Thaleops_v2simp_morph2_unordered_treeLikelihood"/>
    
    <!-- Likelihood of Thaleops_v2simp_morph3_unordered on the tree -->
    <distribution idref="Thaleops_v2simp_morph3_unordered_treeLikelihood"/>
    
    <!-- Likelihood of Thaleops_v2simp_morph4_unordered on the tree -->
    <distribution idref="Thaleops_v2simp_morph4_unordered_treeLikelihood"/>
   </distribution>
  </distribution>
  
  
  
  <!-- Operators to change the dates of the (individual) fossil tipdates -->
  
  
  <!-- XML SECTION 14: Operators. These specify how the parameter values can change at MCMC time-steps. -->
  
  <!-- Additional operators -->
  <operator id="TipDatesRandomWalker_Raymondapsis" windowSize="8.785" taxonset="@age_of_Raymondapsis" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_lacertus" windowSize="3.48499999999999" taxonset="@age_of_T_lacertus" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_viator" windowSize="1.34" taxonset="@age_of_T_viator" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_utahensis" windowSize="1.59999999999999" taxonset="@age_of_T_utahensis" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_marginalis" windowSize="3.63499999999999" taxonset="@age_of_T_marginalis" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_conradi" windowSize="1.80499999999998" taxonset="@age_of_T_conradi" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_angusticollis" windowSize="0.549999999999983" taxonset="@age_of_T_angusticollis" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_depressicapitatus" windowSize="2.72999999999999" taxonset="@age_of_T_depressicapitatus" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_latiaxiatus" windowSize="4.16" taxonset="@age_of_T_latiaxiatus" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_fieldi" windowSize="0.75" taxonset="@age_of_T_fieldi" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_conifrons" windowSize="2.86999999999998" taxonset="@age_of_T_conifrons" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_raymondi" windowSize="1.54999999999998" taxonset="@age_of_T_raymondi" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_mackenziensis" windowSize="0.609999999999985" taxonset="@age_of_T_mackenziensis" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_aduncus" windowSize="0.509999999999991" taxonset="@age_of_T_aduncus" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_longispina" windowSize="1.51999999999998" taxonset="@age_of_T_longispina" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_anusacermissima" windowSize="1.83500000000001" taxonset="@age_of_T_anusacermissima" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_laurentiana" windowSize="3.065" taxonset="@age_of_T_laurentiana" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_jaanussoni" windowSize="2.58499999999998" taxonset="@age_of_T_jaanussoni" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_T_mobydicki" windowSize="2.58499999999998" taxonset="@age_of_T_mobydicki" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  
  
  <!-- Operators on Strict Clock (strict) rate (mean): shared_clock -->
  
  <!-- Operators on the clock mean -->
  <operator id="strictMean_of_shared_clock_scaler" parameter="@strictMean_of_shared_clock" scaleFactor="0.5" spec="ScaleOperator" weight="3.0"/>
  <operator id="strictMean_of_shared_clock_relaxedUpDownOperator" scaleFactor="0.75" spec="UpDownOperator" weight="3.0">
   <parameter idref="strictMean_of_shared_clock" name="up"/>
   <tree idref="shared_tree" name="down"/>
  </operator>
  <operator id="ClockRateScaler_shared_clock" parameter="@strictMean_of_shared_clock" spec="ScaleOperator" weight="3.0" scaleFactor="0.5"/>
  
  <!-- Operator for scaling the gamma shape parameter for morphology -->
  <operator id="morph_gammaShape_Scaler" parameter="@morph_gammaShape" scaleFactor="0.5" spec="ScaleOperator" weight="0.1"/>
  
  <!-- Operators on the tree model parameters -->
  <!-- (commented out unless specified as estimated in Excel settings file) -->
  <operator id="rateScaler_birthRate_shared_tree" parameter="@birthRate" scaleFactor="0.75" weight="10.0" spec="ScaleOperator"/>
  <operator id="rateScaler_deathRate_shared_tree" parameter="@deathRate" scaleFactor="0.75" weight="10.0" spec="ScaleOperator"/>
  <operator id="rateScaler_samplingRate_shared_tree" parameter="@samplingRate" scaleFactor="0.75" weight="10.0" spec="ScaleOperator"/>
  <!-- <operator id="rateScaler_rho_shared_tree" parameter="@rho" scaleFactor="0.75" weight="10.0" spec="ScaleOperator"/> -->
  <operator id="rateScaler_originTime_shared_tree" parameter="@originTime" scaleFactor="0.75" weight="1.0" spec="ScaleOperator"/>
  
  <!-- Operators on the phylogeny (topology and node dates; tip dates could be added) -->
  <operator id="treeScaler_shared_tree" tree="@shared_tree" scaleFactor="0.5" weight="3.0" spec="ScaleOperator"/>
  <operator id="treeRootScaler_shared_tree" tree="@shared_tree" scaleFactor="0.5" weight="3.0" spec="ScaleOperator"/>
  <operator id="UniformOperator_shared_tree" tree="@shared_tree" weight="30.0" spec="Uniform"/>
  <operator id="SubtreeSlide_shared_tree" tree="@shared_tree" weight="15.0" spec="SubtreeSlide"/>
  <operator id="narrowExchange_shared_tree" tree="@shared_tree" weight="15.0" spec="Exchange"/>
  <operator id="wideExchange_shared_tree" tree="@shared_tree" isNarrow="false" weight="3.0" spec="Exchange"/>
  
  <!-- Not sure what this does: NJM 2014-12-01 -->
  <operator id="WilsonBalding_shared_tree" tree="@shared_tree" weight="3.0" spec="WilsonBalding"/>
  
  
  <logger id="tracelog" fileName="traceLog2.txt" logEvery="50000" model="@posterior" sanitiseHeaders="true" sort="smart">
   <log idref="posterior"/>
   <log idref="prior"/>
   <log idref="likelihood"/>
   
   <!-- Log the sampled (individual) tipdates and the prior probability densities (prP) of them -->
   
   
   <!-- XML SECTION 15: Trace log. This logs the parameters in a .log file, which may be viewed in Tracer. -->
   <!-- Reporting node dates/monophyly to trace log -->
   <log idref="total_group_LCA_prior"/>
   <log idref="ingroup1_prior"/>
   
   <!-- Additional logs -->
   <log idref="prP_UniformDistrib_Raymondapsis"/>
   <log idref="prP_UniformDistrib_T_lacertus"/>
   <log idref="prP_UniformDistrib_T_viator"/>
   <log idref="prP_UniformDistrib_T_utahensis"/>
   <log idref="prP_UniformDistrib_T_marginalis"/>
   <log idref="prP_UniformDistrib_T_conradi"/>
   <log idref="prP_UniformDistrib_T_angusticollis"/>
   <log idref="prP_UniformDistrib_T_depressicapitatus"/>
   <log idref="prP_UniformDistrib_T_latiaxiatus"/>
   <log idref="prP_UniformDistrib_T_fieldi"/>
   <log idref="prP_UniformDistrib_T_conifrons"/>
   <log idref="prP_UniformDistrib_T_raymondi"/>
   <log idref="prP_UniformDistrib_T_mackenziensis"/>
   <log idref="prP_UniformDistrib_T_aduncus"/>
   <log idref="prP_UniformDistrib_T_longispina"/>
   <log idref="prP_UniformDistrib_T_anusacermissima"/>
   <log idref="prP_UniformDistrib_T_laurentiana"/>
   <log idref="prP_UniformDistrib_T_jaanussoni"/>
   <log idref="prP_UniformDistrib_T_mobydicki"/>
   
   <!-- Trace log of the &apos;shared_clock&apos;clock model -->
   <parameter idref="strictMean_of_shared_clock" name="log"/>
   
   <!-- Log of rate statistics: rate.mean, rate.variance, rate.coefficientOfVariation -->
   <!-- coefficient of variation = stdev / mean = sqrt(variance)/mean -->
   <log id="rate_summary_shared_clock" branchratemodel="@shared_clock" tree="@shared_tree" spec="beast.evolution.branchratemodel.RateStatistic"/>
   
   <!-- Log of the prior probability density of the sampled mean of this clock -->
   <log idref="prP_UniformDistrib_on_mean_of_shared_clock"/>
   
   
   <log idref="strictMean_of_shared_clock"/>
   
   <!-- Likelihood of morphology data given tree, model, morphology model parameters -->
   <log idref="Thaleops_v2simp_morph2_unordered_treeLikelihood"/>
   <log idref="Thaleops_v2simp_morph3_unordered_treeLikelihood"/>
   <log idref="Thaleops_v2simp_morph4_unordered_treeLikelihood"/>
   
   <!-- Log of the gamma shape parameter for morphology -->
   <log idref="morph_gammaShape"/>
   
   <!-- Log of prior density of the gamma shape parameter for morphology -->
   <log idref="morph_gammaShape_prior"/>
   
   <!-- Log the probability of the tree given the treeModel parameters-->
   <log idref="BDSKY_prior_shared_tree"/>
   
   <!-- Log the prior probability of the treeModel parameters-->
   <log idref="originTime"/>
   <log idref="birthRate"/>
   <log idref="deathRate"/>
   <log idref="samplingRate"/>
   <log idref="rho"/>
   
   <!-- Log of the value of the parameter &apos;birthRate&apos; -->
   
   <!-- Log of the prior probability density of value of the parameter &apos;birthRate&apos; -->
   <log idref="prP_UniformDistrib_on_param_birthRate"/>
   
   <!-- Log of the value of the parameter &apos;deathRate&apos; -->
   
   <!-- Log of the prior probability density of value of the parameter &apos;deathRate&apos; -->
   <log idref="prP_UniformDistrib_on_param_deathRate"/>
   
   <!-- Log of the value of the parameter &apos;samplingRate&apos; -->
   
   <!-- Log of the prior probability density of value of the parameter &apos;samplingRate&apos; -->
   <log idref="prP_UniformDistrib_on_param_samplingRate"/>
  </logger>
  
  
  <logger id="screenlog" fileName="" logEvery="50000" sanitiseHeaders="true" sort="smart">
   <log idref="posterior"/>
   <log idref="prior"/>
   <log idref="likelihood"/>
   
   
   <!-- XML SECTION 16: Screen log. Which parameters print to screen, and how often? -->
   
   <!-- Reporting node dates/monophyly to screen log -->
   <log idref="total_group_LCA_prior"/>
   <log idref="ingroup1_prior"/>
   
   <!-- Additional logs -->
   
   <!-- log of the clock mean for model &apos;shared_clock&apos; -->
   <parameter idref="strictMean_of_shared_clock" name="log"/>
   
   <!-- Likelihood of morphology data given tree, model, morphology model parameters -->
   <log idref="Thaleops_v2simp_morph2_unordered_treeLikelihood"/>
   <log idref="Thaleops_v2simp_morph3_unordered_treeLikelihood"/>
   <log idref="Thaleops_v2simp_morph4_unordered_treeLikelihood"/>
   
   <!-- Log the probability of the tree given the treeModel parameters-->
