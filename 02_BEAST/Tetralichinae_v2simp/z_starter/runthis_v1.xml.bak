  <operator id="rateScaler_deathRate_shared_tree" parameter="@deathRate" scaleFactor="0.75" weight="10.0" spec="ScaleOperator"/>
  <operator id="rateScaler_samplingRate_shared_tree" parameter="@samplingRate" scaleFactor="0.75" weight="10.0" spec="ScaleOperator"/>
  <!-- <operator id="rateScaler_rho_shared_tree" parameter="@rho" scaleFactor="0.75" weight="10.0" spec="ScaleOperator"/> -->
  <operator id="rateScaler_originTime_shared_tree" parameter="@originTime" scaleFactor="0.75" weight="1.0" spec="ScaleOperator"/>
  
  <!-- Operators on the phylogeny (topology and node dates; tip dates could be added) -->
  <operator id="treeScaler_shared_tree" tree="@shared_tree" scaleFactor="0.5" weight="3.0" spec="ScaleOperator"/>
  <operator id="treeRootScaler_shared_tree" tree="@shared_tree" scaleFactor="0.5" weight="3.0" spec="ScaleOperator"/>
  <operator id="UniformOperator_shared_tree" tree="@shared_tree" weight="30.0" spec="Uniform"/>
  <operator id="SubtreeSlide_shared_tree" tree="@shared_tree" weight="15.0" spec="SubtreeSlide"/>
  <operator id="narrowExchange_shared_tree" tree="@shared_tree" weight="15.0" spec="Exchange"/>
  <operator id="wideExchange_shared_tree" tree="@shared_tree" isNarrow="false" weight="3.0" spec="Exchange"/>
  
  <!-- Not sure what this does: NJM 2014-12-01 -->
  <operator id="WilsonBalding_shared_tree" tree="@shared_tree" weight="3.0" spec="WilsonBalding"/>
  
  
  <logger id="tracelog" fileName="traceLog2.txt" logEvery="50000" model="@posterior" sanitiseHeaders="true" sort="smart">
   <log idref="posterior"/>
   <log idref="prior"/>
   <log idref="likelihood"/>
   
   <!-- Log the sampled (individual) tipdates and the prior probability densities (prP) of them -->
   
   
   <!-- XML SECTION 15: Trace log. This logs the parameters in a .log file, which may be viewed in Tracer. -->
   <!-- Reporting node dates/monophyly to trace log -->
   <log idref="total_group_LCA_prior"/>
   <log idref="ingroup1_prior"/>
   
   <!-- Additional logs -->
   <log idref="prP_UniformDistrib_P_kristiae"/>
   <log idref="prP_UniformDistrib_P_pandus"/>
   <log idref="prP_UniformDistrib_P_robbinsi"/>
   <log idref="prP_UniformDistrib_A_lineatus"/>
   <log idref="prP_UniformDistrib_A_subpunctatus"/>
   <log idref="prP_UniformDistrib_A_aspratilis"/>
   <log idref="prP_UniformDistrib_A_conradi"/>
   <log idref="prP_UniformDistrib_A_cucullus"/>
   <log idref="prP_UniformDistrib_A_jukesi"/>
   <log idref="prP_UniformDistrib_L_bronnikovi"/>
   <log idref="prP_UniformDistrib_A_encyrtos"/>
   <log idref="prP_UniformDistrib_A_effnensis"/>
   
   <!-- Trace log of the &apos;shared_clock&apos;clock model -->
   <parameter idref="strictMean_of_shared_clock" name="log"/>
   
   <!-- Log of rate statistics: rate.mean, rate.variance, rate.coefficientOfVariation -->
   <!-- coefficient of variation = stdev / mean = sqrt(variance)/mean -->
   <log id="rate_summary_shared_clock" branchratemodel="@shared_clock" tree="@shared_tree" spec="beast.evolution.branchratemodel.RateStatistic"/>
   
   <!-- Log of the prior probability density of the sampled mean of this clock -->
   <log idref="prP_UniformDistrib_on_mean_of_shared_clock"/>
   
   
   <log idref="strictMean_of_shared_clock"/>
   
   <!-- Likelihood of morphology data given tree, model, morphology model parameters -->
   <log idref="Tetralichinae_v2simp_morph2_unordered_treeLikelihood"/>
   <log idref="Tetralichinae_v2simp_morph3_unordered_treeLikelihood"/>
   <log idref="Tetralichinae_v2simp_morph4_unordered_treeLikelihood"/>
   
   <!-- Log of the gamma shape parameter for morphology -->
   <log idref="morph_gammaShape"/>
   
   <!-- Log of prior density of the gamma shape parameter for morphology -->
   <log idref="morph_gammaShape_prior"/>
   
   <!-- Log the probability of the tree given the treeModel parameters-->
   <log idref="BDSKY_prior_shared_tree"/>
   
   <!-- Log the prior probability of the treeModel parameters-->
   <log idref="originTime"/>
   <log idref="birthRate"/>
   <log idref="deathRate"/>
   <log idref="samplingRate"/>
   <log idref="rho"/>
   
   <!-- Log of the value of the parameter &apos;birthRate&apos; -->
   
   <!-- Log of the prior probability density of value of the parameter &apos;birthRate&apos; -->
   <log idref="prP_UniformDistrib_on_param_birthRate"/>
   
   <!-- Log of the value of the parameter &apos;deathRate&apos; -->
   
   <!-- Log of the prior probability density of value of the parameter &apos;deathRate&apos; -->
   <log idref="prP_UniformDistrib_on_param_deathRate"/>
   
   <!-- Log of the value of the parameter &apos;samplingRate&apos; -->
   
   <!-- Log of the prior probability density of value of the parameter &apos;samplingRate&apos; -->
   <log idref="prP_UniformDistrib_on_param_samplingRate"/>
  </logger>
  
  
  <logger id="screenlog" fileName="" logEvery="50000" sanitiseHeaders="true" sort="smart">
   <log idref="posterior"/>
   <log idref="prior"/>
   <log idref="likelihood"/>
   
   
   <!-- XML SECTION 16: Screen log. Which parameters print to screen, and how often? -->
   
   <!-- Reporting node dates/monophyly to screen log -->
   <log idref="total_group_LCA_prior"/>
   <log idref="ingroup1_prior"/>
   
   <!-- Additional logs -->
   
   <!-- log of the clock mean for model &apos;shared_clock&apos; -->
   <parameter idref="strictMean_of_shared_clock" name="log"/>
   
   <!-- Likelihood of morphology data given tree, model, morphology model parameters -->
   <log idref="Tetralichinae_v2simp_morph2_unordered_treeLikelihood"/>
   <log idref="Tetralichinae_v2simp_morph3_unordered_treeLikelihood"/>
   <log idref="Tetralichinae_v2simp_morph4_unordered_treeLikelihood"/>
   
   <!-- Log the probability of the tree given the treeModel parameters-->
   <log idref="BDSKY_prior_shared_tree"/>
   
   <!-- Log the prior probability of the treeModel parameters-->
   <log idref="originTime"/>
   <log idref="birthRate"/>
   <log idref="deathRate"/>
   <log idref="samplingRate"/>
   <log idref="rho"/>
  </logger>
  
  
  <logger id="treelog" fileName="treeLog2.txt" logEvery="50000" mode="tree">
   
   
   <!-- XML SECTION 17: Tree log. How often should time trees from the MCMC chain be saved, and what data should they have? -->
   <!--       (these are trees where the branch lengths are in time units, e.g. millions of years )                    -->
   <!--   NOTE: this file can get HUGE if you sample too frequently. You just want a few thousand samples at the end.       -->
   
   <!-- Log the tree and branch rates -->
   <log id="TreeWithMetaDataLogger_shared_tree" tree="@shared_tree" branchratemodel="@shared_clock" substitutions="false" spec="beast.evolution.tree.TreeWithMetaDataLogger"/>
  </logger>
  
  
  <logger id="subslog" fileName="subsLog2.txt" logEvery="50000" mode="tree">
   
   
   <!-- XML SECTION 18: Substitutions Tree log. How often should these trees be saved, and what data should they have?      -->
   <!--         (these are trees where the branch lengths are the expected number of substitutions, i.e. rate * time)      -->
   <!--   NOTE: this file can get HUGE if you sample too frequently. You just want a few thousand samples at the end.      -->
   
   <!-- Log the tree and branch rates -->
   <log id="Subs_TreeWithMetaDataLogger_shared_tree" tree="@shared_tree" substitutions="true" spec="beast.evolution.tree.TreeWithMetaDataLogger"/>
  </logger>
  
  
  
  
 </run>
</beast>
