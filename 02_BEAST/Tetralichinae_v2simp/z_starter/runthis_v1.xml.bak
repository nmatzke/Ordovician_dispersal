 <BirthDeathSkylineModel id="@shared_tree" tree="@shared_tree" contemp="false" spec="beast.evolution.speciation.BirthDeathSkylineModel">
  
  <!-- Starting parameters of the BirthDeathSkylineModel for the observed tree -->
  
  <!-- originTime: the start of the sampling+tree growth process -->
  <!-- (This is a nuisance parameter, but its starting value must be older than the start tree root or Beast2 will crash) -->
  <parameter id="originTime" lower="0.0" upper="4600" name="origin">100</parameter>
  
  <!-- birthRates (lambda: speciation or lineage splitting rates, oldest to youngest) -->
  <parameter id="birthRate" name="birthRate" lower="0" upper="9" value="0.02"/>
  <parameter id="birthRateChangeTimes_tops" name="birthRateChangeTimes" value="0"/>
  
  <!-- deathRates (mu: lineage extinction rates, oldest to youngest) -->
  <parameter id="deathRate" name="deathRate" lower="0" upper="1" value="0.002"/>
  <parameter id="deathRateChangeTimes_tops" name="deathRateChangeTimes" value="0"/>
  
  <!-- samplingRates (psi: rate of sampling per lineage per my)                  -->
  <!-- sorted from youngest to oldest, but with zero at the end,                 -->
  <!-- as specified by Denise on the beast-users group:                          -->
  <!-- https://groups.google.com/d/msg/beast-users/UyAS0sYDkKk/PB0ktWnssn0J          -->
  <!-- The actual psi samplingRates, though, are in order from oldest to youngest -->
  <parameter id="samplingRate" name="samplingRate" lower="0" upper="9" value="0.002"/>
  <parameter id="samplingRateChangeTimes_tops" name="samplingRateChangeTimes" value="0"/>
  
  <!-- rho (probability of sampling each lineage extant at a particular timepoint, typically the present, 0 Ma) -->
  <parameter id="rho" name="rho" lower="0.0" value="1"/>
  <parameter id="rhoSamplingTimes" name="rhoSamplingTimes" value="0"/>
  
  <!-- Are time arrays reversed, for birth, death, psi, rho, and R. Regardless, the parameter values are listed from oldest to youngest time bin. -->
  <reverseTimeArrays id="reverseTimeArrays" spec="beast.core.parameter.BooleanParameter" value="true true true true true"/>
 </BirthDeathSkylineModel>
 
 
