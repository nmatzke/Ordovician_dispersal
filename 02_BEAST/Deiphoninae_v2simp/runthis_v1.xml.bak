  <operator id="TipDatesRandomWalker_D_longifrons" windowSize="3" taxonset="@age_of_D_longifrons" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_D_fleur" windowSize="8.155" taxonset="@age_of_D_fleur" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_S_robustus" windowSize="10.9" taxonset="@age_of_S_robustus" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_S_kingi" windowSize="5.155" taxonset="@age_of_S_kingi" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_S_dentata" windowSize="10.05" taxonset="@age_of_S_dentata" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_S_longispina" windowSize="2.05000000000001" taxonset="@age_of_S_longispina" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_S_cranium" windowSize="1.04999999999998" taxonset="@age_of_S_cranium" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_S_maquoketensis" windowSize="6.20000000000002" taxonset="@age_of_S_maquoketensis" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_S_murphyi" windowSize="1" taxonset="@age_of_S_murphyi" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_S_gemina" windowSize="1.40000000000001" taxonset="@age_of_S_gemina" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_S_goodnovi" windowSize="1.59999999999999" taxonset="@age_of_S_goodnovi" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_S_exserta" windowSize="3.56" taxonset="@age_of_S_exserta" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_H_huebneri" windowSize="1.89000000000001" taxonset="@age_of_H_huebneri" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_H_pseudohemicranium" windowSize="0.920000000000016" taxonset="@age_of_H_pseudohemicranium" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
