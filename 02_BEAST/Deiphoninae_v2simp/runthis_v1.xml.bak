  <operator id="TipDatesRandomWalker_Actinopeltis_carolialexandri" windowSize="0.775000000000006" taxonset="@age_of_Actinopeltis_carolialexandri" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_D_barrandei" windowSize="3.10000000000002" taxonset="@age_of_D_barrandei" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_D_globifrons" windowSize="2.05000000000001" taxonset="@age_of_D_globifrons" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_D_ellipticum" windowSize="3.09999999999999" taxonset="@age_of_D_ellipticum" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
  <operator id="TipDatesRandomWalker_D_bainsi" windowSize="6.5" taxonset="@age_of_D_bainsi" tree="@shared_tree" weight="1.0" spec="beast.evolution.operators.TipDatesRandomWalker"/>
