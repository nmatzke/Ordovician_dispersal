    <prior id="prP_UniformDistrib_on_param_strictMean_of_shared_clock" name="distribution" x="@strictMean_of_shared_clock">
     <Uniform id="UniformDistrib_on_param_strictMean_of_shared_clock" name="distr" offset="0" lower="0" upper="10"/>
    </prior>
    
    <!-- Prior on parameter(s) for morphology evolution model -->
    
    <!-- Prior density of the gamma shape parameter for morphology -->
    <prior id="morph_gammaShape_prior" distr="@exponential_distrib_for_gammaShape_priors" name="distribution" x="@morph_gammaShape"/>
    
    <!-- Prior probability distributions of tree model parameters -->
    
    <!-- Uniform prior on the time of origin of the process (assumes my) -->
    <!-- x means whatever parameter you are assessing via the distribution -->
    <prior id="prior_originTime" name="distribution" x="@originTime">
     <Uniform id="prior_originTime_Uniform_distrib" lower="0.0" upper="4600.0" name="distr"/>
    </prior>
    
    <!-- Priors on birthRate(s) -->
    
    <!-- Prior probability density on the value of the parameter &apos;birthRate&apos;, according to a uniform distribution with lower=0, upper=9. -->
    <prior id="prP_UniformDistrib_on_param_birthRate" name="distribution" x="@birthRate">
     <Uniform id="UniformDistrib_on_param_birthRate" name="distr" offset="0" lower="0" upper="9"/>
    </prior>
    
    <!-- Priors on deathRate(s) -->
    
    <!-- Prior probability density on the value of the parameter &apos;deathRate&apos;, according to a uniform distribution with lower=0, upper=9. -->
    <prior id="prP_UniformDistrib_on_param_deathRate" name="distribution" x="@deathRate">
