 <BirthDeathSkylineModel id="@shared_tree" tree="@shared_tree" contemp="false" spec="beast.evolution.speciation.BirthDeathSkylineModel">
  
  <!-- Starting parameters of the BirthDeathSkylineModel for the observed tree -->
  
  <!-- originTime: the start of the sampling+tree growth process -->
  <!-- (This is a nuisance parameter, but its starting value must be older than the start tree root or Beast2 will crash) -->
  <parameter id="originTime" lower="0.0" upper="4600" name="origin">100</parameter>
  
